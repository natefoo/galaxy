[env]
PYTHONPATH=lib

[watcher:web]
cmd = gunicorn 'galaxy.webapps.reports.fast_factory:factory()' --timeout 300 --pythonpath lib -k galaxy.webapps.galaxy.workers.Worker -b fd://$(circus.sockets.web)
send_hup = true
numprocesses = 1
use_sockets = True
stop_signal = TERM
stop_children = True
copy_env = True

[socket:web]
host = 127.0.0.1
port = 9001
